{
  "manifest_version": 3,
  "name": "s.cybozu.com Redirect",
  "version": "0.0.0",
  "description": "Redirect s.cybozu.com to cybozu.com",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "ruleset.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://*.s.cybozu.com/*",
    "https://*.s.cybozu-dev.com/*",
    "https://*.s.cybozu.cn/*",
    "https://*.s.cybozu-dev.cn/*"
  ]
}
